package 연습하기;

import java.util.Scanner;
import java.util.Stack;

public class 괄호 {

	//같은 괄호가 나오면 뺴주고 , 다른 괄호가 나오면 지워준다. 그리고 Stack 에 아무것도 없으면 VPS이다.
	
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int num = scan.nextInt();
		Stack<Character> stack = new Stack<>();
		boolean isc = true;
		while(num--> 0) {
		
			String s = scan.next();
					
			for(char c : s.toCharArray()) {
						
				if(c=='(') {
					stack.add(c);
				}else {
					
					if(!stack.isEmpty()) {
						stack.pop();
					}else {
						isc = false;
						break;
					}
				}
					
									
			}
			
					
		}	

	}

}
