import java.util.Arrays;

public class 정요섭 {
	
	public static void main(String[] args) {
		
		
		int a = 5;
		int b = a;
		
		b=3;
		System.out.println(a);
		System.out.println(b);
		
		int[] c = {1,2,3};
		int[] d = new int[c.length];
			
		
		d[0]=3;
		
		System.out.println(Arrays.toString(c));
		System.out.println(Arrays.toString(d));
	}

}

